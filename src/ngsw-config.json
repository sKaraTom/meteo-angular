{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html"
      ],
      "versionedFiles": [
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [ "/assets/**" ]
    }
  }],
  "DataGroups": [
    {
      "name": "openweatheramp",
      "urls": ["/accueil"],
      "cacheConfig": { 
        "strategy": "freshness",  
        "maxSize": 20,  
        "maxAge": "1h",     
        "timeout": "5s"
      }
    }
  ]
}